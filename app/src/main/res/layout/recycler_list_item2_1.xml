<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="235dp"
    android:translationY="-15dp">

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:cardCornerRadius="18dp"
        app:cardElevation="10dp"
        app:cardUseCompatPadding="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="387dp"
            android:layout_height="197dp"
            android:orientation="vertical"
            android:padding="20dp">

            <LinearLayout
                android:layout_width="346dp"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/sidoImageId"
                    android:layout_width="33dp"
                    android:layout_height="33dp"
                    app:srcCompat="@mipmap/ic_launcher" />

                <TextView
                    android:id="@+id/sidoTextViewId"
                    android:layout_width="290dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:fontFamily="@font/hs"
                    android:paddingStart="24dp"
                    android:paddingLeft="4dp"
                    android:text="city"
                    android:textAppearance="@style/TextAppearance.AppCompat.Large"
                    android:textSize="18dp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="311dp"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:padding="3dp"
                android:translationX="5dp"
                android:translationY="5dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/hs"
                    android:paddingStart="5dp"
                    android:paddingLeft="24dp"
                    android:paddingTop="4dp"
                    android:paddingBottom="4dp"
                    android:text="1차 누적"
                    android:textSize="15dp" />

                <TextView
                    android:id="@+id/accumulate1TextViewId"
                    android:layout_width="155dp"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/hs"
                    android:paddingStart="24dp"
                    android:paddingLeft="24dp"
                    android:paddingTop="4dp"
                    android:paddingBottom="4dp"
                    android:textColor="@color/red"
                    android:textSize="15dp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="310dp"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:padding="3dp"
                android:translationX="5dp"
                android:translationY="5dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/hs"
                    android:paddingStart="5dp"
                    android:paddingLeft="24dp"
                    android:paddingTop="4dp"
                    android:paddingBottom="4dp"
                    android:text="1차 신규"
                    android:textSize="15dp" />

                <TextView
                    android:id="@+id/new1TextViewId"
                    android:layout_width="155dp"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/hs"
                    android:paddingStart="24dp"
                    android:paddingLeft="24dp"
                    android:paddingTop="4dp"
                    android:paddingBottom="4dp"
                    android:textColor="@color/red"
                    android:textSize="15dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="310dp"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:padding="3dp"
                android:translationX="5dp"
                android:translationY="5dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/hs"
                    android:paddingStart="5dp"
                    android:paddingLeft="24dp"
                    android:paddingTop="4dp"
                    android:paddingBottom="4dp"
                    android:text="2차 누적"
                    android:textSize="15dp" />

                <TextView
                    android:id="@+id/accumulate2TextViewId"
                    android:layout_width="153dp"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/hs"
                    android:paddingStart="24dp"
                    android:paddingLeft="24dp"
                    android:paddingTop="4dp"
                    android:paddingBottom="4dp"
                    android:textColor="@color/blue"
                    android:textSize="15dp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="311dp"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:padding="3dp"
                android:translationX="5dp"
                android:translationY="5dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/hs"
                    android:paddingStart="5dp"
                    android:paddingLeft="24dp"
                    android:paddingTop="4dp"
                    android:paddingBottom="4dp"
                    android:text="2차 신규"
                    android:textSize="15dp" />

                <TextView
                    android:id="@+id/new2TextViewId"
                    android:layout_width="151dp"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/hs"
                    android:paddingStart="24dp"
                    android:paddingLeft="24dp"
                    android:paddingTop="4dp"
                    android:paddingBottom="4dp"
                    android:textColor="@color/blue"
                    android:textSize="15dp" />
            </LinearLayout>

        </LinearLayout>
    </androidx.cardview.widget.CardView>

</RelativeLayout>